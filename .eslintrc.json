{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "settings": {
    "react": {
      "pragma": "React",
      "version": "0.14.3"
    }
  },
  "rules": {
    "no-console": 1,
    "no-empty": 0,
    "no-dupe-args": 2,
    "no-duplicate-case": 2,
    "no-extra-parens": 0,
    "no-func-assign": 1,
    "react/jsx-key": 1,
    "no-dupe-keys": 2,
    "no-eval": 2,
    "react/jsx-no-duplicate-props": 2,
    "no-undefined": 0,
    "constructor-super": 2,
    "no-const-assign": 2
  }
}